{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "lint": "tslint ./src/**/*.ts -t verbose",
    "start": "ng serve",
    "test": "ng test",
    "build": "ng build --prod",
    "e2e": "ng e2e",
    "ng": "ng",
    "hero-row-build": "ng build hero-row --single-bundle",
    "hero-row-copy-dist-to-heroes": "cpx dist/hero-row/*es2015.js src/assets",
    "hero-row-copy-dist-to-simple": "cpx dist/hero-row/*es2015.js simple-usage-example && cpx node_modules/zone.js/dist/zone.js simple-usage-example",
    "hero-row-build-and-copy": "npm run hero-row-build && npm run hero-row-copy-dist-to-simple && npm run hero-row-copy-dist-to-heroes"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^8.2.11",
    "@angular/common": "^8.2.11",
    "@angular/compiler": "^8.2.11",
    "@angular/core": "^8.2.11",
    "@angular/elements": "^8.2.11",
    "@angular/forms": "^8.2.11",
    "@angular/platform-browser": "^8.2.11",
    "@angular/platform-browser-dynamic": "^8.2.11",
    "@angular/router": "^8.2.11",
    "@angular/upgrade": "^8.2.11",
    "core-js": "^2.5.4",
    "rxjs": "^6.5.3",
    "zone.js": "~0.9.1",
    "document-register-element": "^1.7.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.13",
    "@angular/cli": "^8.3.13",
    "@angular/compiler-cli": "^8.2.11",
    "@angular/platform-server": "^8.2.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~8.9.4",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.16.2",
    "ngx-build-plus": "^9.0.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "repository": {}
}
